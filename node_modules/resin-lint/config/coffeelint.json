{
    "coffeescript_error": {
        "level": "error"
    },
    "arrow_spacing": {
        "level": "error"
    },
    "no_tabs": {
        "level": "ignore"
    },
    "no_trailing_whitespace": {
        "level": "error",
        "allowed_in_comments": false,
        "allowed_in_empty_lines": false
    },
    "max_line_length": {
        "value": 180,
        "level": "error",
        "limitComments": true
    },
    "no_trailing_semicolons": {
        "level": "error"
    },
    "indentation": {
        "value": 1,
        "level": "error"
    },
    "camel_case_classes": {
        "level": "error"
    },
    "colon_assignment_spacing": {
        "level": "error",
        "spacing": {
            "left": 0,
            "right": 1
        }
    },
    "no_implicit_braces": {
        "level": "ignore",
        "strict": false
    },
    "no_plusplus": {
        "level": "ignore"
    },
    "no_throwing_strings": {
        "level": "error"
    },
    "no_backticks": {
        "level": "error"
    },
    "no_implicit_parens": {
        "strict": false,
        "level": "ignore"
    },
    "no_empty_param_list": {
        "level": "error"
    },
    "no_stand_alone_at": {
        "level": "error"
    },
    "space_operators": {
        "level": "error"
    },
    "duplicate_key": {
        "level": "error"
    },
    "empty_constructor_needs_parens": {
        "level": "warn"
    },
    "cyclomatic_complexity": {
        "value": 10,
        "level": "warn"
    },
    "newlines_after_classes": {
        "value": 1,
        "level": "error"
    },
    "no_unnecessary_fat_arrows": {
        "level": "error"
    },
    "missing_fat_arrows": {
        "level": "ignore"
    },
    "non_empty_constructor_needs_parens": {
        "level": "ignore"
    },
    "no_unnecessary_double_quotes": {
        "level": "error"
    },
    "no_debugger": {
        "level": "warn"
    },
    "no_interpolation_in_single_quotes": {
        "level": "error"
    },
    "braces_spacing": {
        "spaces": 1,
        "level": "error"
    },
    "spacing_after_comma": {
        "level": "error"
    },
    "check_scope": {
      "module": "coffeescope2",
      "level": "warn",
      "environments": [ "builtin", "es5", "es6", "node", "mocha", "browser", "amd" ],
      "globals": {},
      "overwrite": false,
      "same_scope": false,
      "shadow": false,
      "shadow_builtins": false,
      "shadow_exceptions": [],
      "undefined": true,
      "hoist_local": true,
      "hoist_parent": true,
      "unused_variables": true,
      "unused_arguments": false
    }
}
